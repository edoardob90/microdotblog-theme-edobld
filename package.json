{
  "name": "microdotblog-theme-edobld",
  "version": "0.3.0",
  "description": "Starter files for a Hugo theme with Tailwindcss",
  "main": "index.js",
  "repository": "https://github.com/edoardob90/microdotblog-theme-edobld",
  "author": "edoardob90 <edoardob90@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "hugo --gc",
    "build:css": "npx tailwindcss -i ./_assets/css/styles.css -o ./assets/css/styles.css",
    "dev:css": "npx tailwindcss --watch -i ./_assets/css/styles.css -o ./assets/css/styles.css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^9.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-go-template": "^0.0.15",
    "tailwindcss": "^3.3.3"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
